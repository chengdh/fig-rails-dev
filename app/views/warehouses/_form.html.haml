= semantic_form_for resource,html: {id: "warehouse_form"} do |f|
  = f.inputs do
    = f.input :org_id,as: :select,collection: current_ability_orgs_for_select,include_blank: false,input_html: {style: "width : 30%;"}
    = f.input :warehouse_type_id,as: :select,collection: warehouse_types_for_select,include_blank: false,input_html: {style: "width : 30%;"}
    = f.input :name,input_html: {style: "width : 30%;"}
    = f.input :address,input_html: {style: "width : 60%;"}
    = f.input :leader,input_html: {style: "width : 60%;"}
    = f.input :phone,input_html: {style: "width : 30%;"}
    = f.input :area,input_html: {style: "width : 30%;"}
    = f.input :volume,input_html: {style: "width : 40%;"}
    = f.input :order_by,input_html: {style: "width : 30%;"}
    = f.input :is_active,input_html: {style: "width : 20%;"}
    = f.input :note,input_html: {rows: 2,style: "width : 40%;"}
