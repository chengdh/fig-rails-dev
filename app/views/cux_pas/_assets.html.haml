%table.table.table-condensed.table-striped.table-hover
  %thead
    %tr
      %th 资产名称
      %th 数量
      -# %th 型号
      %th{:style => "width: 20%;"} 价值
      %th{:style => "width: 20%;"} 资产类别
      %th{:style => "width: 20%;"} 启用日期
  %tbody
    - @cux_pa.cux_pa_trast_headers.each_with_index do |line,i|
      %tr
        %td
          =line.asset_name
        %td
          =line.asset_quantity
        %td
          =line.cost
        %td
          =line.asset_num
        %td
          =line.date_placed_in_service.strftime("%Y-%m-%d")
